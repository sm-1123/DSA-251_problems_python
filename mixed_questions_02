P7:Flatten Binary Tree to Linked List[TC:O(N),SC:O(1)]
def flattenBinaryTree(root):
    # Write your code here.
    curr = root

    while(curr):

        if curr.left:

            prev = curr.left

            while(prev.right):

                prev = prev.right

            prev.right = curr.right

            curr.right = curr.left

        curr = curr.right        

    return root    

P8:Rearrange Linked List[TC:O(N),SC:O(1)]
def rearrangeList(head):
    # Write your code here.
    if not head or not head.next:

        return head    

    def reversed_ll(node):

        prev = None

        while node:

            temp = node.next

            node.next = prev

            prev = node

            node = temp

        return prev

    slow, fast = head, head

    while fast and fast.next:

        slow = slow.next

        fast = fast.next.next

    node2 = reversed_ll(slow.next)

    slow.next = None

    node1 = head

    dummy = Node(0)

    curr = dummy

    while(node1 or node2):

        if node1:

            curr.next = node1

            curr = curr.next

            node1 = node1.next

        if node2:

            curr.next = node2

            curr = curr.next

            node2 = node2.next             

    return dummy.next
    
    pass

P9: Sorted Linked List to Balanced BST[TC:O(N), SC:O(N)]
def fnc(arr, l, r):

	if l > r:

		return None

	mid = (l + r)//2

	node = Node(arr[mid])

	node.left = fnc(arr, l, mid - 1)

	node.right = fnc(arr, mid + 1, r)

	return node	

def sortedListToBST(head):

	arr = []

	while head:

		arr.append(head.data)

		head = head.next

	return fnc(arr, 0, len(arr) - 1)



